{
  "name": "klimatkoll",
  "version": "0.0.1",
  "description": "",
  "main": "dist/game-server.js",
  "scripts": {
    "test": "mocha --inline-diffs --bail --require ts-node/register --watch-extensions ts --watch --watch-files src './src/**/*.spec.ts'",
    "coverage": "nyc --reporter=html --reporter=text mocha --require ts-node/register ./src/**/*.spec.ts",
    "dev": "tsc --build --watch & nodemon dist/dev.js",
    "start": "node compiled/index.js",
    "build": "tsc --build && rm -r dist/public && cp -r public dist/public",
    "create-ca": "node node_modules/mkcert/src/cli.js create-ca",
    "create-cert": "node node_modules/mkcert/src/cli.js create-cert",
    "setup-https": "npm run create-ca && npm run create-cert"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "basic-auth": "^2.0.1",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-fileupload": "^1.4.0",
    "images": "^3.2.4",
    "mocha": "^9.1.2",
    "pug": "^3.0.2",
    "rxjs": "^6.6.3",
    "seedrandom": "^3.0.5",
    "sqlite3": "^5.0.8",
    "uniqid": "^5.4.0",
    "websocket": "^1.0.32",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@types/basic-auth": "^1.1.3",
    "@types/chai": "^4.3.0",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.9",
    "@types/express-fileupload": "^1.2.2",
    "@types/mocha": "^9.1.1",
    "@types/node": "^14.14.20",
    "@types/seedrandom": "^2.4.28",
    "@types/sqlite3": "^3.1.8",
    "@types/uniqid": "^5.3.2",
    "@types/websocket": "^1.0.1",
    "@types/yauzl": "^2.10.0",
    "chai": "^4.3.4",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.1.3"
  }
}
